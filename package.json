    "@superwall/react-native-superwall": "^2.0.0",
    "convex": "^1.19.2",
    "date-fns": "^4.1.0",
    "expo": "~53.0.0",
    "expo-auth-session": "~6.3.15",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "^5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
