{
  "name": "unbet",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "node scripts/smart-clean.js && expo run:ios",
    "ios:clean": "npm run clean && expo run:ios",
    "ios:force": "node scripts/smart-clean.js --force && expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "prebuild": "npx convex codegen",
    "eas-build-pre-install": "npx convex codegen",
    "clean": "npm run clean:all",
    "clean:all": "npm run clean:watchman && npm run clean:metro && npm run clean:ios",
    "clean:watchman": "watchman watch-del-all 2>/dev/null || true",
    "clean:metro": "rm -rf node_modules/.cache/metro && rm -rf .expo",
    "clean:ios": "cd ios && rm -rf build Pods ~/Library/Developer/Xcode/DerivedData/* && pod cache clean --all && cd ..",
    "reinstall": "rm -rf node_modules && npm install && cd ios && pod install && cd ..",
    "fresh": "npm run clean:all && npm run reinstall"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@auth/core": "^0.37.0",
    "@babel/runtime": "^7.28.2",
    "@clerk/clerk-expo": "^2.8.5",
    "@convex-dev/auth": "^0.0.87",
